<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_shoppage_cover" name="shop Page Banner">
   <section class="s_shoppage_cover banner">
   
    <span style="background-image: url('/Suryaweb/static/src/img/Shop_Page_banner_img.png'); background-position: 50% 0;"/>
        <div class="o_we_bg_filter bg-black-50"/>
        <div class="container s_allow_columns">
            <h1 style="text-align: center; margin-top:110px;"><font style="font-weight: bold;">Chic &amp; Cosy <span>Rugs,</span> for interiors<br></br> with <span>unique aesthetics,</span> <br></br>like you</font></h1>
        </div></section>
</template>

<template id="place_into_bar_shoppage_cover" inherit_id="website.snippets" name="Place into bar">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="Suryaweb.s_shoppage_cover"/>
        </xpath>

</template>

</odoo>
