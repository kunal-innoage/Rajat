<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="s_style_products" name="Product Style">
        <section class="s_style_products">
            <div id="formList">
                <div id="list">
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/orient.jpg" class="avatar"/>
                        <div class="content">
                   1001 Nights Atmosphere
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/modern.jpg" class="avatar"/>
                        <div class="content"> 
                   Contemporary Chic
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/boho.jpg" class="avatar"/>
                        <div class="content">
                   Nomad-inspired Designs
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/hygge.jpg" class="avatar"/>
                        <div class="content">
                   Slow Living
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/industrial.jpg" class="avatar"/>
                        <div class="content">
                   Artistâ€™s Studio Spirit
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/retro.jpg" class="avatar"/>
                        <div class="content">
                   Glam Never Gets Old
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/pop.jpg" class="avatar"/>
                        <div class="content">
                   For a Lively interior
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/chick_plain.jpg" class="avatar"/>
                        <div class="content">
                   Modern-Day Monochrome
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/ethnic.jpg" class="avatar"/>
                        <div class="content">
                   Journey Beyond the Horizon
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/wabi_sabi.jpg" class="avatar"/>
                        <div class="content">
                   Elusive Beauty of Imperfection
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/japandi.jpg" class="avatar"/>
                        <div class="content">
                   Zen and Minimalist
                        </div>
                    </div>
                    <div class="item">
                        <img src="/Suryaweb/static/src/img/cutie_kidos.jpg" class="avatar"/>
                        <div class="content">
                   Joyful and Fun
                        </div>
                    </div>
                </div>
            </div>


            <div class="direction">
                <button id="prev"> &lt;</button>
                <button id="next"> &gt;</button>
            </div>


        </section>
    </template>

    <template id="place_into_bar_style_products" inherit_id="website.snippets" name="Place into bar">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="Suryaweb.s_style_products"/>
        </xpath>
    </template>

</odoo>
